Algoritmo ejercicio06
	Definir dolar Como Entero
	Definir pesos, conversion Como Real
	
	Escribir "Ingrese la cantidad de pesos a convertir:";
	Leer pesos;
	
	dolar <- 985;
	
	conversion <- pesos / dolar;
	
	Escribir "La conversion de $", pesos, "ARS a dolar es: $", conversion, " USD";
FinAlgoritmo
