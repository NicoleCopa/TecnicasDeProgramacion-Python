Algoritmo ejercicio02
	//Entrada
	Escribir("Ingrese un número:")
	Leer num1
	Escribir("Ingrese un segundo número")
	Leer num2
	//Proceso
	temp <- num1
	num1 <- num2
	num2 <- temp
	//Salida
	Escribir ("El primer número es:")
	Escribir (num1)
	Escribir ("El segundo número es:")
	Escribir (num2)
FinAlgoritmo
